{
  "name": "kryuu-common/big-chain-db-driver",
  "description": "Base58 Encoding and Decoding Library for PHP",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/KatsuoRyuu/php-Base58"
    },
    {
      "type": "vcs",
      "url": "https://github.com/KatsuoRyuu/php-JsonStableStringify"
    },
    {
      "type": "vcs",
      "url": "https://github.com/KatsuoRyuu/php-Buffer"
    },
    {
      "type": "vcs",
      "url": "https://github.com/KatsuoRyuu/php-CryptoConditions"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "kryuu-common/buffer": "dev-master",
    "kryuu-common/crypto-conditions": "dev-master",
    "kryuu-common/json-stable-stringify": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "KryuuCommon\\BigChainDb\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KryuuCommon\\BigChainDbTest\\": "test/"
    }
  }
}
